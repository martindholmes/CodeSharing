              <ref name="tei_editor"/>
              <ref name="tei_sponsor"/>
              <ref name="tei_funder"/>
              <ref name="tei_respStmt"/>
            </choice>
          </zeroOrMore>
        </zeroOrMore>
        <ref name="tei_imprint"/>
        <zeroOrMore>
          <choice>
            <ref name="tei_imprint"/>
            <ref name="tei_extent"/>
            <ref name="tei_biblScope"/>
          </choice>
